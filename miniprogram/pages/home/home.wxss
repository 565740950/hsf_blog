.home .swiper {
  height: 300rpx;
}

.home .swiper .slide-image {
  width: 100%;
  height: 300rpx;
}

.home .classfication {
  display: flex;
  /* background:#fff; */
  flex-direction: row;
  justify-content: space-around;
  margin: 15rpx 0;
}

.home .classfication .classfication-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.home .classfication .classfication-view .image {
  width: 120rpx;
  height: 120rpx;
  background: #fefe;
  border-radius: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.home .classfication .classfication-view .image image {
  width: 60rpx;
  height: 60rpx;
}

.home .classfication .classfication-view .name {
  font-size: 25rpx;
  margin-top: 5rpx;
}
